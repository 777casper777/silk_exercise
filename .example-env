# MongoDB connection URI (used inside Docker)
MONGO_URI=mongodb://mongo:27017

# Redis connection URI (used inside Docker)
REDIS_URL=redis://redis:6379/0

# Whether to enable TTL (Time To Live) for Redis deduplication keys
USE_REDIS_TTL=true

# TTL duration in seconds for Redis keys (7 days = 604800 seconds)
DEDUP_TTL=604800

# API token for external data sources
API_TOKEN=
